{
  //The 'manifest_version' key must be present and set to 2 (without quotes)
  "manifest_version": 2,
  "name": "Copy Fixer",
  "version": "1.0.3",
  "description": "Copy title and URL using Control/Command-C.",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "32": "icon32.png",
      "64": "icon64.png"
   },
   "content_scripts": [
    {
      "all_frames": true,
      "matches": ["http://*/*", "https://*/*"],
      "js": ["copyfixer.js"],
      "run_at": "document_end"
    }
  ],
  //background_page' requires manifest version of 1 or lower.
  //"background_page": "background.html"
  "background":
  {
      "page": "background.html"
  }
}
