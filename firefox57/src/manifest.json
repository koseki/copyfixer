{
  "manifest_version": 2,
  "name": "Copy Fixer",
  "version": "1.1.3",
  "description": "Copy title and URL using Control/Command-C.",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "96": "icon96.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["<all_urls>"],
      "js": ["copyfixer.js"],
      "run_at": "document_end"
    }
  ]
}
